											<xs:documentation>Individal</xs:documentation>
										</xs:annotation>
									</xs:enumeration>
									<xs:enumeration value="U.S. Bank">
										<xs:annotation>
											<xs:documentation>U.S. Bank</xs:documentation>
										</xs:annotation>
									</xs:enumeration>
									<xs:enumeration value="Non-U.S. Bank">
										<xs:annotation>
											<xs:documentation>Non-U.S. Bank</xs:documentation>
										</xs:annotation>
									</xs:enumeration>
									<xs:enumeration value="U.S. Non-Bank">
										<xs:annotation>
											<xs:documentation>U.S. Non-Bank</xs:documentation>
										</xs:annotation>
									</xs:enumeration>
									<xs:enumeration value="Non-U.S. Non-Bank">
