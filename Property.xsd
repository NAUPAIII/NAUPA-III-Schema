											<xs:documentation>Income Tax Withheld</xs:documentation>
										</xs:annotation>
									</xs:enumeration>
									<xs:enumeration value="IRS Penalty withheld">
										<xs:annotation>
											<xs:documentation>IRS Penalty withheld</xs:documentation>
										</xs:annotation>
									</xs:enumeration>
									<xs:enumeration value="IRS Back-Up Withholding">
										<xs:annotation>
											<xs:documentation>Due Diligence Cost</xs:documentation>
										</xs:annotation>
									</xs:enumeration>
									<!-- TODO:  others? -->
								</xs:restriction>
							</xs:simpleType>
                        </xs:element>
                        <xs:element name="Amount" type="CurrencyType">
                            <xs:annotation>
                                <xs:documentation>
                                    Dollar amount of the adjustment resulting in net amount reported.
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
