            <xs:element name="PresumedAbandonedDate" type="DateType">
                <xs:annotation>
                    <xs:documentation>
                        Date on which the holder determined the property to be unclaimed (as contrasted with the
                        date on which the property became reportable).
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="DueDiligence" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        <!-- TODO:  description -->
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Date1" type="DateType" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>
                                    Date that statutorily required owner notification initially performed.
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="Date2" type="DateType" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>
                                    Date(s) that supplemental owner notification(s) performed, if needed.
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="ReturnedByPostOffice" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        <!-- TODO -->
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="MailDate" type="DateType">
                            <xs:annotation>
                                <xs:documentation>
                                    Date that statutorily required owner notification initially performed.
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="ReturnCount" type="xs:integer">
                            <xs:annotation>
                                <xs:documentation>
                                    <!-- TODO -->
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="DateAccountCodedRPO" type="DateType">
                            <xs:annotation>
