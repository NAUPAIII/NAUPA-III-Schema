            <xs:element name="Adjustment" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType >
                    <xs:sequence>
                        <xs:element name="Type">
							<xs:simpleType>
								<xs:annotation>
									<xs:documentation>
										<!-- TODO:  description -->
									</xs:documentation>
								</xs:annotation>
