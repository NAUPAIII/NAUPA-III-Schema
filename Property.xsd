<?xml version="1.0" encoding="UTF-8"?>
<xs:schema targetNamespace="http://www.unclaimed.org/NAUPA-3"
           xmlns="http://www.unclaimed.org/NAUPA-3"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           elementFormDefault="qualified">
           
    <xs:include schemaLocation="Common.xsd" />
    <xs:include schemaLocation="Owner.xsd" />
    
    <!--
    Named simple types section _NSTS
    -->
    
    <xs:simpleType name="SecurityNameType">
        <xs:annotation>
            <xs:documentation>
                Name of security.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <!-- Based on Avenu -->
            <xs:maxLength value="50" />
        </xs:restriction>
    </xs:simpleType>
    
    <xs:simpleType name="SecuritySymbolType">
        <xs:annotation>
            <xs:documentation>
                Ticker symbol for a security.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <!-- Based on Avenu -->
            <xs:maxLength value="6" />
        </xs:restriction>
    </xs:simpleType>
    
    <xs:simpleType name="CUSIPType">
        <xs:annotation>
            <xs:documentation>
                A CUSIP is a nine-digit numeric (e.g. 037833100 for Apple) or nine-character alphanumeric (e.g.
                38259P508 for Google) code that identifies a North American financial security for the purposes
                of facilitating clearing and settlement of trades. The acronym CUSIP derives from the Committee
                on Uniform Security Identification Procedures, a committee of the American Bankers Association.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <!-- Based on Avenu -->
            <xs:maxLength value="12" />
        </xs:restriction>
    </xs:simpleType>
    
    <xs:simpleType name="NumberOfSharesType">
        <xs:restriction base="xs:decimal">
            <xs:fractionDigits value="3" />
        </xs:restriction>
    </xs:simpleType>
    
    <xs:simpleType name="AlternativeSecurityIdentifierType">
        <xs:restriction base="xs:string">
            <!-- Based on Avenu -->
            <xs:maxLength value="20" />
        </xs:restriction>
    </xs:simpleType>
    
    <xs:simpleType name="AlternativeSecurityIDTypeType">
        <xs:annotation>
            <xs:documentation>
            <!-- TODO:  description -->
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="SEDOL">
                <xs:annotation>
                    <xs:documentation>SEDOL</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="ISIN">
                <xs:annotation>
                    <xs:documentation>ISIN</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="VALOREN">
                <xs:annotation>
                    <xs:documentation>VALOREN</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    
    <!--
    Complex types section _CTS
    -->
    
    <xs:complexType name="OFACInformationType">
        <!-- If this information is present, are all elements required? If not, which ones are?  As currently defined
             here, they would all be required or the XML would fail validation.  -->
        <xs:sequence>
            <xs:element name="Description">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <!-- TODO:  define max length
                        <xs:maxLength value="?" />
                         -->
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="DateOfBlocking" type="DateType">
                <xs:annotation>
                    <xs:documentation>
                        <!-- TODO:  do we really need yet another date format? -->
                        The date (YYYY-MM-DD) the property was blocked, if available.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="LegalAuthority">
                <xs:annotation>
                    <xs:documentation>
                        Cite the legal authority or authorities under which the property is blocked(e.g., 31 C.F.R.
                        Part 515).
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <!-- TODO:  define max length
                        <xs:maxLength value="?" />
                         -->
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="SanctionsTarget">
                <xs:annotation>
                    <xs:documentation>
                        The associated sanctions target whose property is blocked, or a reference to the
                        relevant communication from OFAC instructing a party to block this property if that
                        target is unknown (e.g. "Cuba").
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <!-- TODO:  define max length
                        <xs:maxLength value="?" />
                         -->
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="OwnerType" type="OFACOwnerType">
                <xs:annotation>
                    <xs:documentation>
                        Indicate owner type, selecting from one of the following categories: individual; U.S. bank;
                        non-U.S. bank; U.S. non-bank entity; non-U.S. non-bank entity; or other.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="CashType">
        <xs:sequence>
            <xs:element name="ReportedAmount" type="CurrencyType">
                <xs:annotation>
                    <xs:documentation>
                        Enter the amount due the owner before any deductions, such as taxes, were subtracted.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="InterestPaidThroughDate" type="DateType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Date through which Interest is included in the Reported Amount.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="InterestRate" type="PercentageType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Interest rate - shown as a percentage.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="RemittedAmount" type="CurrencyType">
                <xs:annotation>
                    <xs:documentation>
                        Amount due to owner (Reported Amount +/- Adjustments).
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="CheckNumber" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Provide the check number if reporting cashier's or traveler's checks, money orders, or any
                        other remittance instrument.
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <!-- Based on Kelmar -->
                        <xs:maxLength value="100" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="ItemIssueState" type="StateCodeType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        For traveler's checks, money orders, cashier's checks, and other remittance instruments,
                        enter the two character state code for the location where the instrument was purchased.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="SecurityAdjustmentType">
        <xs:sequence>
            <xs:element name="Type" type="SecurityAdjustmentTypeType">
                <xs:annotation>
                    <xs:documentation>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Amount" type="CurrencyType">
                <xs:annotation>
                    <xs:documentation>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="CostBasisType">
        <xs:sequence>
            <xs:element name="SecurityName" type="SecurityNameType">
                <xs:annotation>
                    <xs:documentation>
                        Enter the name of the security issue which is due the owner. DO NOT enter more than one
                        issue per owner record. (For example, brokers must provide one property record for each
                        issue held in an owner's account.) When reporting stock where the owner has different
                        issues and these issues will be due the owner, there should be a property record for each
                        issue that will be due that owner. For example: Paul Smith had an account with XYZ broker.
                        In the account, Mr. Smith owned 12 shares of ABC Office Supply Common, 4 shares of ABC
                        Office Supply $2.75 Preferred, and 20 shares of Afterschool Snack Company Common. There
                        should be three property records for Mr. Smith.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SecuritySymbol" type="SecuritySymbolType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Enter Security Ticker Symbol - Enter the Ticker Symbol under which the security is being
                        traded - if available
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="CUSIP" type="CUSIPType">
                <xs:annotation>
                    <xs:documentation>
                        Enter the CUSIP number of the specific security issue. If no CUSIP number available,
                        contact State's custodian to obtain placeholder CUSIP number.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="AlternativeSecurityIDType" type="AlternativeSecurityIDTypeType"  minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="AlternativeSecurityIdentifier" type="AlternativeSecurityIdentifierType"  minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        For securities that don’t have a CUSIP - maybe use SEDOL OR ISIN?
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Amount" type="CurrencyType">
                <xs:annotation>
                    <xs:documentation>
                        The amount paid for the security
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="NumberofSharesPurchased" type="NumberOfSharesType">
                <xs:annotation>
                    <xs:documentation>
                        Number of shares purchased in this transaction
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="PurchaseDate" type="DateType">
                <xs:annotation>
                    <xs:documentation>
                        Date original purchase occurred
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <!-- TODO:  refine this -->
            <xs:element name="LongTermShortTerm" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Provide this information as defined by Internal Revenue Code of United States - Need
                        more clarification as how this field will be used to arrive at a better description.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="SecuritiesType">
        <xs:sequence>
            <xs:element name="TransferAgent">
                <xs:annotation>
                    <xs:documentation>
                        Transfer Agent is any entity that delivers securities to the State - including but not
                        limited to Transfer Agents, Brokers, Mutual Funds, Privately held companies, etc.
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <!-- Based on Avenu -->
                        <xs:maxLength value="35" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="SecurityName" type="SecurityNameType">
                <xs:annotation>
                    <xs:documentation>
                        Enter the name of the security issue which is due the owner. DO NOT enter more than one
                        issue per owner record. (For example, brokers must provide one property record for each
                        issue held in an owner's account.)  When reporting stock where the owner has different
                        issues and these issues will be due the owner, there should be a property record for each
                        issue that will be due that owner. For example: Paul Smith had an account with XYZ broker.
                        In the account, Mr. Smith owned 12 shares of ABC Office Supply Common, 4 shares of ABC
                        Office Supply $2.75 Preferred, and 20 shares of Afterschool Snack Company Common. There
                        should be three property records for Mr. Smith.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SecuritySymbol" type="SecuritySymbolType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Enter Security Ticker Symbol - Enter the Ticker Symbol under which the security is being
                        traded - if available.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="CUSIP" type="CUSIPType">
                <xs:annotation>
                    <xs:documentation>
                        Enter the CUSIP number of the specific security issue. If no CUSIP number available,
                        contact State's custodian to obtain placeholder CUSIP number.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="AlternativeSecurityIDType" type="AlternativeSecurityIDTypeType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Please enter ISIN, Valoren, SEDOL.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="AlternativeSecurityIdentifier" type="AlternativeSecurityIdentifierType">
                <xs:annotation>
                    <xs:documentation>
                        Enter the Security Identifier issued by entity indicated by the previous field. Any
                        security identifier that the holder has such but not limited to SEDOL/ISIN/Valoren, etc.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ReportedShares" type="NumberOfSharesType">
                <xs:annotation>
                    <xs:documentation>
                        Enter number of shares including fractional shares. Note that most processing platforms
                        can only accommodate only up to three decimal places. Decimal required if reporting
                        fractional shares.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="RemittedShares" type="NumberOfSharesType">
                <xs:annotation>
                    <xs:documentation>
                        Final number of shares remitted to state. Decimal required if remitting fractional shares,
                        up to three decimal places. Note that most processing platforms can only accommodate only
                        up to three decimal places.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="NameOfOriginalSharesHeld" type="SecurityNameType">
                <xs:annotation>
                    <xs:documentation>
                        Enter the original security name (This is for  when the stock went through some merger and
                        the property owner owned the previous stock but is being reported under the current name).
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="NumberOfOriginalSharesHeld" type="NumberOfSharesType">
                <xs:annotation>
                    <xs:documentation>
                        Enter number of unexchanged shares. Decimal required if reporting fractional shares. Note
                        that most processing platforms can only accommodate only up to three decimal places.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="CUSIPOfOriginalSharesHeld" type="CUSIPType">
                <xs:annotation>
                    <xs:documentation>
                        Enter the CUSIP number of the security having unexchanged shares. This CUSIP # should
                        reflect the CUSIP # associated with the field "Original Shares Held - Name" above.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Type" type="SecurityTypeType">
                <xs:annotation>
                    <xs:documentation>
                        Stock, Mutual Funds, Bonds, Warrants, Rights, Privately held, CVR, ADR, Foreign.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <!-- TODO:  discuss renaming of element -->
            <xs:element name="DividendHandling" type="DividendHandlingType">
                <xs:annotation>
                    <xs:documentation>
                        <!-- TODO:  review description -->
                        Provide the method of payment for Dividends and Capital Gains (short/long term) - cash
                        or re-invest- can we add a field for Capital Gain same as the one for the Dividends? -
                        Capital Gains - short/long - how they are handled.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="DeliveryMethod" type="DeliveryMethodType">
                <xs:annotation>
                    <xs:documentation>
                        <!-- TODO:  review description -->
                        DTC type of movements (DO, DWAC, DRS), Foreign, FED, Physical, Statement.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="StateAccountNumber">
                <xs:annotation>
                    <xs:documentation>
                        This is the State account number into which the securities are delivered.
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <!-- Based on Avenu -->
                        <xs:maxLength value="25" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="DTCDeliveryReferenceNumber">
                <xs:annotation>
                    <xs:documentation>
                        Please include DTC (Depository Trust Company)'s Delivery confirmation number for stocks
                        delivered through DTC as well as State Account number if DRS (Direct Registered Shares),
                        State account numbers for Mutual Funds, Certificate number and registration for physical
                        certificates.
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <!-- Based on Avenu -->
                        <xs:maxLength value="20" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="Market" type="SecurityMarketType">
                <xs:annotation>
                    <xs:documentation>
                        Market Exchange where the security is held and traded.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="DividendsPaidThroughDate" type="DateType">
                <xs:annotation>
                    <xs:documentation>
                        <!-- TODO:  review description -->
                        Date through which dividend and other payments - Can we develop a new field for Long/Short
                        term capital gains?-  were reported for this security.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SECSearch1date" type="DateType">
                <xs:annotation>
                    <xs:documentation>
                        Need clarification for the purpose of this field.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SECSearch2date" type="DateType">
                <xs:annotation>
                    <xs:documentation>
                        Need clarification for the purpose of this field.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Adjustment" type="SecurityAdjustmentType">
                <xs:annotation>
                    <xs:documentation>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="CostBasis" type="CostBasisType">
                <xs:annotation>
                    <xs:documentation>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="SafekeepingFeeType">
        <xs:sequence>
            <xs:element name="Type" type="SafekeepingFeeTypeType">
                <xs:annotation>
                    <xs:documentation>
                        Type of safekeeping fee.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Amount" type="CurrencyType">
                <xs:annotation>
                    <xs:documentation>
                        Safekeeping fee amount.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="SavingsBondOwnerType">
        <xs:sequence>
            <xs:element name="Name">
                <xs:annotation>
                    <xs:documentation>
                        Listed Payee for bond.
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <!-- Based on Kelmar -->
                        <xs:maxLength value="250" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="Address" type="AddressType">
                <xs:annotation>
                    <xs:documentation>
                        Address for bond payee.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SocialSecurityNumber" type="SSNType">
                <xs:annotation>
                    <xs:documentation>
                        Social security number or bond payee.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Relationship" type="SavingsBondOwnerRelationshipType">
                <xs:annotation>
                    <xs:documentation>
                        Relationship of owner listed to this bond - new table (owner, purchaser, POD, etc.).
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="SavingsBondType">
        <xs:sequence>
            <xs:element name="BondNumber">
                <xs:annotation>
                    <xs:documentation>
                        Savings Bond registration number.
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <!-- Based on Kelmar -->
                        <xs:maxLength value="250" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="FaceValue" type="CurrencyType">
                <xs:annotation>
                    <xs:documentation>
                        Face value for savings bond.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Owner" type="SavingsBondOwnerType">
                <xs:annotation>
                    <xs:documentation>
                        Owner information for savings bond.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="SafekeepingInventoryType">
        <xs:sequence>
            <xs:element name="InventoryItemCode" type="SafekeepingInventoryItemType">
                <xs:annotation>
                    <xs:documentation>
                        Tangible category code - contact each state for valid code listing.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="InventoryItemDescription">
                <xs:annotation>
                    <xs:documentation>
                        Description of contents in Safe Deposit Box or Container.
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <!-- Based on Kelmar -->
                        <xs:maxLength value="2000" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="SavingsBond" type="SavingsBondType" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>
                        Savings Bond item in safekeeping.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="SafekeepingType">
        <xs:sequence>
            <xs:element name="BoxCaseNumber">
                <xs:annotation>
                    <xs:documentation>
                        Safe Deposit Box Number or Container Number.
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <!-- Based on Kelmar -->
                        <xs:maxLength value="25" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="BoxOpenDate" type="DateType">
                <xs:annotation>
                    <xs:documentation>
                        Date Box was drilled open.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="LeaseExpiredDate" type="DateType">
                <xs:annotation>
                    <xs:documentation>
                        End date on the paid rental agreement.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Fee" type="SafekeepingFeeType">
                <xs:annotation>
                    <xs:documentation>
                        Safekeeping fee.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Inventory" type="SafekeepingInventoryType" minOccurs="1" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>
                        Safekeeping inventory item.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    
    <!--  TODO:  all of this looks very Oklahoma-specific.  Do we need to generalize? -->
    <xs:complexType name="MineralInterestsType">
        <xs:sequence>
            <xs:element name="StateCode" type="StateCodeType">
                <xs:annotation>
                    <xs:documentation>
                        STATE-CODE - State abbreviation of the state where the mineral interest property is located.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="StateFormatNumber" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        <!-- TODO:  do we really need this? -->
                        STATE-FORMAT-NUM - This is not used in the application. 01 can be used in this field.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="CountyName">
                <xs:annotation>
                    <xs:documentation>
                        COUNTY-NAME - Geographical region of a state used for administrative or other purposes.
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <!-- From FieldDescriptions in spreadsheet -->
                        <xs:maxLength value="20" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="Section">
                <xs:annotation>
                    <xs:documentation>
                        SECTION - An area nominally one square mile, containing 640 acres, with 36 sections
                        making up one survey township on a rectangular grid.
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <!-- From FieldDescriptions in spreadsheet -->
                        <xs:maxLength value="2" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="Township">
                <xs:annotation>
                    <xs:documentation>
                        TOWNSHIP - The Township is a division of a county. Township grid lines run North and South.
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <!-- From FieldDescriptions in spreadsheet -->
                        <xs:maxLength value="3" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="Range">
                <xs:annotation>
                    <xs:documentation>
                        RANGE - The range number identifies how many cells the property is to the East or West of
                        a starting point. Range grid lines run East and West.
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <!-- From FieldDescriptions in spreadsheet -->
                        <xs:maxLength value="3" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="SupplementalDescription">
                <xs:annotation>
                    <xs:documentation>
                        SUPP-DESC - Description of the quarter (NE Quarter, South Quarter), probate number,
                        deed information (county book and page reference, fractional share or co-ownership
                        (1/8th), etc.
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <!-- From FieldDescriptions in spreadsheet -->
                        <xs:maxLength value="20" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="PoolingOrder">
                <xs:annotation>
                    <xs:documentation>
                        POOLING-ORDER - A specific number assigned to a Pooling action. A Pooling is the
                        consolidation and combining of leased land with adjoining leased tracts. The area is
                        called a pool or a unit. Pooling has the benefit to the production company of uniting
                        all landowners' leases into a common pool under one drilling production company and 
                        utilizing one common underground geological reservoir. The pooling action may be
                        voluntary or forced (unlocated owners).
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <!-- From FieldDescriptions in spreadsheet -->
                        <xs:maxLength value="6" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="PoolingDate" type="DateType">
                <xs:annotation>
                    <xs:documentation>
                        POOLING-DATE - Date signed by the Corporation Commission.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="WellName">
                <xs:annotation>
                    <xs:documentation>
                        WELL-NAME -Wells are traditionally named for the mineral owner(s) in the drilling unit.
                        [e.g. Smith or Smith and Jones].If the State owns minerals in the drilling unit, the well
                        name shall be "State" followed by the name of the township where the well is located [e.g.
                        State Chester]. If the Federal government owns minerals, the well name shall include "USA"
                        followed by the name of the township where the well is located [e.g. USA Hillman].
                </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <!-- From FieldDescriptions in spreadsheet -->
                        <xs:maxLength value="30" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="WellNumber">
                <xs:annotation>
                    <xs:documentation>
                        WELL-NUMBER - An American Petroleum Institute (API) well number or API number is a "unique,
                        permanent, numeric identifier" assigned to each well drilled for oil and gas in the United
                        States.
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <!-- From FieldDescriptions in spreadsheet -->
                        <xs:maxLength value="20" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="LeaseName">
                <xs:annotation>
                    <xs:documentation>
                        LEASE- NAME - An oil lease is an agreement between parties to allow a Lessee to have access
                        to the property and minerals on the property of the Lessor. The agreement contains the
                        decided upon lease name.
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <!-- From FieldDescriptions in spreadsheet -->
                        <xs:maxLength value="30" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="LeaseNumber">
                <xs:annotation>
                    <xs:documentation>
                        LEASE-NUMBER - The lease number serves as identification keys for well bores (actual hole
                        that forms a well) and completions.
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <!-- From FieldDescriptions in spreadsheet -->
                        <xs:maxLength value="20" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="OwnershipPercentage" type="PercentageType">
                <xs:annotation>
                    <xs:documentation>
                        OWNERSHIP-PCT - The owner's proportional ownership in the minerals (reflected as a decimal
                        interest).
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

