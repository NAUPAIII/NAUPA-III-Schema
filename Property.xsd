            <xs:element name="LocationIdentifier">
                <xs:annotation>
                    <xs:documentation>
                        Indicate whether location of reporting company is a branch, franchise office,
                        specific unit etc if different than main address.
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="20" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <!-- TODO:  can these be foreign addresses? -->
            <xs:element name="LocationAddress">
				<xs:complexType>
					<xs:choice>
						<xs:element name="USAddress" type="USAddressType" />
						<xs:element name="ForeignAddress" type="ForeignAddressType" />
