    <!-- TODO:  add CryptoCurrency here -->
    
    <xs:complexType name="PropertyType">
        <xs:sequence>
            <xs:element name="HIPAAIndicator" type="IndicatorType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Property information contains information subject to HIPAA regulations.  Presence of element
                        containing the value "X" indicates the presence of HIPPA information, absence of such an
                        elements indicates no HIPPA information.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="AccountNumber">
                <xs:annotation>
                    <xs:documentation>
                        Account number, policy number (incurance), owner number (minerals), box number (tangibles)
                        or other unique identifier under which the holder identifies the owner--if there are
                        multiple numbers used to identify the owner, include all such numbers. If additional space
                        is needed, use Miscellaneous Description field.
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <!-- Based on Avenu -->
                        <xs:maxLength value="25" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <!-- TODO:  see TODOs in PropertyTypeCodeType -->
