  <!-- Section 5 Security Element Types-->
  <!-- Section 5.1 Security Structural Element Types -->
  <!-- Section 5.1.1 AlternativeSecurityIDType -->
  <xs:complexType name="AlternativeSecurityIDType">
    <xs:sequence>
      <!-- Section 5.1.1.1 AlternativeSecurityIDType TypeCode -->
      <xs:element name="TypeCode">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="10" />
            <xs:enumeration value="SEDOL" />
            <xs:enumeration value="ISIN" />
            <xs:enumeration value="VALOREN" />
            <xs:enumeration value="OTHER" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="Identifier">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="20" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <!-- Section 5.2 Security Data Element Types-->
  <xs:simpleType name="SecurityCUSIPType">
    <xs:restriction base="xs:string">
      <xs:maxLength value="9" />
      <xs:pattern value="[A-Za-z0-9]{8}[0-9]{1}" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="SecurityNameType">
    <xs:restriction base="xs:string">
      <xs:maxLength value="100" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="SecurityNumberOfSharesType">
    <xs:restriction base="xs:decimal">
      <xs:totalDigits value="12" />
      <xs:fractionDigits value="6" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="SecuritySymbolType">
    <xs:restriction base="xs:string">
      <xs:maxLength value="10" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Section 6 Cryptocurrency Element Types-->
  <!-- Section 6.1 Cryptocurrency Data Element Types -->
  <xs:simpleType name="CryptocurrencyUnits">
    <xs:restriction base="xs:integer">
      <xs:totalDigits value="12" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Section 7 Common Element Types -->
  <!-- Section 7.1 Common Structural Element Types -->
  <!-- Section 7.1.1 ContactType -->
  <xs:complexType name="ContactType">
    <xs:sequence>
      <!-- Section 7.1.1.1 ContactType TypeCode -->
      <xs:element name="TypeCode">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="50" />
            <xs:enumeration value="Claims" />
            <xs:enumeration value="Report" />
            <xs:enumeration value="CFO" />
            <xs:enumeration value="Safe Deposit Box" />
            <xs:enumeration value="Securities" />
