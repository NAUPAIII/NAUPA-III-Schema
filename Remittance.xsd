<?xml version="1.0" encoding="UTF-8"?>
<xs:schema targetNamespace="http://www.unclaimed.org/NAUPA-III"
  xmlns="http://www.unclaimed.org/NAUPA-III"
  xmlns:xs="http://www.w3.org/2001/XMLSchema"
  elementFormDefault="qualified">

  <!-- Section 4 Remittance Element -->
  <xs:element name="Remittance">
    <xs:complexType>
      <xs:sequence>
        <!-- Section 4.1 SoftwareInformation Element -->
        <xs:element name="SoftwareInformation">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Version">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="50" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="Contact" type="ContactType" />
            </xs:sequence>
          </xs:complexType>
        </xs:element> <!-- SoftwareInformation -->
        <!-- Section 4.2 Payment Element -->
        <xs:element name="Payment" minOccurs="0">
          <xs:complexType>
            <xs:sequence>
              <!-- Section 4.2.1 Payment TypeCode -->
              <xs:element name="TypeCode" minOccurs="0">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="50" />
                    <xs:enumeration value="Online" />
                    <xs:enumeration value="Check" />
                    <xs:enumeration value="Wire" />
                    <xs:enumeration value="ACH Credit" />
                    <xs:enumeration value="ACH Debit" />
                    <xs:enumeration value="Interagency" />
                    <xs:enumeration value="ECheck" />
                    <xs:enumeration value="Other" />
                    <xs:enumeration value="None" /> <!-- ??? Leave out Payment element if there is no payment -->
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="ConfirmationNumber">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="50"/>
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element> <!-- Payment -->
        <!-- Section 4.3 Remitter Element -->
        <xs:element name="Remitter">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="CompanyName" type="CompanyNameType" />
              <xs:choice>
                <xs:element name="USCompanyInfo" type="USCompanyInfoType" />
                <xs:element name="ForeignCompanyInfo" type="ForeignCompanyInfoType" />
              </xs:choice>
              <xs:element name="TelephoneNumber" type="TelephoneNumberType" />
              <xs:element name="EMailAddress" type="EMailAddressType" />
            </xs:sequence>
          </xs:complexType>
        </xs:element> <!-- Remitter -->
        <!-- Section 4.4 Holder Element -->
        <xs:element name="Holder" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="CompanyName" type="CompanyNameType" />
              <xs:choice>
                <xs:element name="USCompanyInfo" type="USCompanyInfoType" />
                <xs:element name="ForeignCompanyInfo" type="ForeignCompanyInfoType" />
              </xs:choice>
              <!-- Section 4.4.1 ParentCompany Element -->
              <xs:element name="ParentCompany" minOccurs="0">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="CompanyName" type="CompanyNameType" />
                    <xs:choice>
                      <xs:element name="FEIN" type="FEINType" />
                      <xs:element name="ForeignTaxID" type="ForeignTaxIDType" />
                    </xs:choice>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="Contact" type="ContactType" maxOccurs="unbounded" />
              <xs:element name="NAICSCode">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="[1-9][1-9][0-9]{0,4}" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="TaxIDExtension" minOccurs="0">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="[0-9]{4}" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="DBAName" type="CompanyNameType" minOccurs="0" maxOccurs="unbounded"/>
              <!-- Section 4.4.2 Incorporated Element -->
              <xs:element name="Incorporated" minOccurs="0">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="State" type="StateCodeType" />
                    <xs:element name="Date" type="xs:date" />
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="SalesAmount" type="CurrencyType" minOccurs="0" />
              <xs:element name="NumberOfEmployees" minOccurs="0">
                <xs:simpleType>
                  <xs:restriction base="xs:integer">
                    <xs:totalDigits value="12" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="AssetsAmount" type="CurrencyType" minOccurs="0" />
              <xs:element name="NumberOfInsurancePolicies" minOccurs="0">
                <xs:simpleType>
                  <xs:restriction base="xs:integer">
                    <xs:totalDigits value="12" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="HolderID" minOccurs="0">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="[0-9]{1,15}" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <!-- Section 4.4.3 Report Element -->
              <xs:element name="Report">
                <xs:complexType>
                  <xs:sequence>
                    <!-- Section 4.4.3.1 Report TypeCode Element -->
                    <xs:element name="TypeCode">
                      <xs:simpleType>
                        <xs:restriction base="xs:string">
                          <xs:maxLength value="50" />
                          <xs:enumeration value="Advertising/No Remittance" />
                          <xs:enumeration value="Remittance/Annual" />
                          <xs:enumeration value="Test" />
                          <xs:enumeration value="Audit" />
                          <xs:enumeration value="Supplemental/Correction" />
                          <xs:enumeration value="Reciprocal" />
                          <xs:enumeration value="Voluntary Disclosure Agreement" />
                          <xs:enumeration value="Final Remittance" />
                          <xs:enumeration value="Initial Remittance" />
                        </xs:restriction>
                      </xs:simpleType>
                    </xs:element>
                    <xs:element name="Year">
                      <xs:simpleType>
                        <xs:restriction base="xs:string">
                          <xs:pattern value="[0-9]{4}" />
                        </xs:restriction>
                      </xs:simpleType>
                    </xs:element>
                    <xs:element name="AsOfDate" type="xs:date" minOccurs="0" />
                    <xs:element name="Contact" type="ContactType" maxOccurs="unbounded" />
                    <!-- Section 4.4.3.2 Property Element -->
                    <xs:element name="Property" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:sequence>
                          <!-- Section 4.4.3.2.1 Property TypeCode Element -->
                          <xs:element name="TypeCode">
                            <xs:simpleType>
                              <xs:restriction base="xs:string">
                                <xs:maxLength value="5" />
                                <xs:enumeration value="AC001" />
                                <xs:enumeration value="AC002" />
                                <xs:enumeration value="AC003" />
                                <xs:enumeration value="AC004" />
                                <xs:enumeration value="AC005" />
                                <xs:enumeration value="AC006" />
                                <xs:enumeration value="AC007" />
                                <xs:enumeration value="AC008" />
                                <xs:enumeration value="AC009" />
                                <xs:enumeration value="AC010" />
                                <xs:enumeration value="AC011" />
                                <xs:enumeration value="AC012" />
                                <xs:enumeration value="CK001" />
                                <xs:enumeration value="CK002" />
                                <xs:enumeration value="CK003" />
                                <xs:enumeration value="CK004" />
                                <xs:enumeration value="CK005" />
                                <xs:enumeration value="CK006" />
                                <xs:enumeration value="CK007" />
                                <xs:enumeration value="CK008" />
                                <xs:enumeration value="CK009" />
                                <xs:enumeration value="CS001" />
                                <xs:enumeration value="CS002" />
                                <xs:enumeration value="CS003" />
                                <xs:enumeration value="CS004" />
                                <xs:enumeration value="CS005" />
                                <xs:enumeration value="CS006" />
                                <xs:enumeration value="CS007" />
                                <xs:enumeration value="CS008" />
                                <xs:enumeration value="CS009" />
                                <xs:enumeration value="CS010" />
                                <xs:enumeration value="CT001" />
                                <xs:enumeration value="CT002" />
                                <xs:enumeration value="CT003" />
                                <xs:enumeration value="CT004" />
                                <xs:enumeration value="CT005" />
                                <xs:enumeration value="CT006" />
                                <xs:enumeration value="CT007" />
                                <xs:enumeration value="CT008" />
                                <xs:enumeration value="CT009" />
                                <xs:enumeration value="CT010" />
                                <xs:enumeration value="CT011" />
                                <xs:enumeration value="HS001" />
                                <xs:enumeration value="HS002" />
                                <xs:enumeration value="IN001" />
                                <xs:enumeration value="IN002" />
                                <xs:enumeration value="IN003" />
                                <xs:enumeration value="IN004" />
                                <xs:enumeration value="IN005" />
                                <xs:enumeration value="IN006" />
                                <xs:enumeration value="IN007" />
                                <xs:enumeration value="IN008" />
                                <xs:enumeration value="IR001" />
                                <xs:enumeration value="IR002" />
                                <xs:enumeration value="IR003" />
                                <xs:enumeration value="IR004" />
                                <xs:enumeration value="IR005" />
                                <xs:enumeration value="IR006" />
                                <xs:enumeration value="IR007" />
                                <xs:enumeration value="IR008" />
                                <xs:enumeration value="IR009" />
                                <xs:enumeration value="MI001" />
                                <xs:enumeration value="MI002" />
                                <xs:enumeration value="MI003" />
                                <xs:enumeration value="MI004" />
                                <xs:enumeration value="MI005" />
                                <xs:enumeration value="MI006" />
                                <xs:enumeration value="MI007" />
                                <xs:enumeration value="MI008" />
                                <xs:enumeration value="MI009" />
                                <xs:enumeration value="MS001" />
                                <xs:enumeration value="MS002" />
                                <xs:enumeration value="MS003" />
                                <xs:enumeration value="MS004" />
                                <xs:enumeration value="MS005" />
                                <xs:enumeration value="MS006" />
                                <xs:enumeration value="MS007" />
                                <xs:enumeration value="MS008" />
                                <xs:enumeration value="MS010" />
                                <xs:enumeration value="MS011" />
                                <xs:enumeration value="MS012" />
                                <xs:enumeration value="MS013" />
                                <xs:enumeration value="SC001" />
                                <xs:enumeration value="SC002" />
                                <xs:enumeration value="SC003" />
                                <xs:enumeration value="SC004" />
                                <xs:enumeration value="SC005" />
                                <xs:enumeration value="SC006" />
                                <xs:enumeration value="SC007" />
                                <xs:enumeration value="SC008" />
                                <xs:enumeration value="SC009" />
                                <xs:enumeration value="SC010" />
                                <xs:enumeration value="SC011" />
                                <xs:enumeration value="SC012" />
                                <xs:enumeration value="SC013" />
                                <xs:enumeration value="SC014" />
                                <xs:enumeration value="SC015" />
                                <xs:enumeration value="SC016" />
                                <xs:enumeration value="SC017" />
                                <xs:enumeration value="SD001" />
                                <xs:enumeration value="SD002" />
                                <xs:enumeration value="SD003" />
                                <xs:enumeration value="TR001" />
                                <xs:enumeration value="TR002" />
                                <xs:enumeration value="TR003" />
                                <xs:enumeration value="TR004" />
                                <xs:enumeration value="TR005" />
                                <xs:enumeration value="TR006" />
                                <xs:enumeration value="TR007" />
                                <xs:enumeration value="UT001" />
                                <xs:enumeration value="UT002" />
                                <xs:enumeration value="VC001" />
                                <xs:enumeration value="VC002" />
                              </xs:restriction>
                            </xs:simpleType>
                          </xs:element>
                          <xs:element name="AccountNumber">
                            <xs:simpleType>
                              <xs:restriction base="xs:string">
                                <xs:maxLength value="100" />
                              </xs:restriction>
                            </xs:simpleType>
                          </xs:element>
                          <xs:element name="PresumedAbandonedDate" type="xs:date" />
                          <!-- Section 4.4.3.2.2 DueDiligencePerformed Element -->
                          <xs:element name="DueDiligencePerformed" minOccurs="0">
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="Date1" type="xs:date" />
                                <xs:element name="Date2" type="xs:date" minOccurs="0" />
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                          <!-- Section 4.4.3.2.3 ReturnedByPostOffice Element -->
                          <xs:element name="ReturnedByPostOffice" minOccurs="0">
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="MailDate" type="xs:date" />
                                <xs:element name="ReturnCount" type="xs:short" minOccurs="0" />
                                <xs:element name="DateAccountCodedRPO" type="xs:date" minOccurs="0" />
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                          <xs:element name="PayableOrDistributableDate" type="xs:date" minOccurs="0" />
                          <xs:choice>
                            <xs:element name="LastActivityDate" type="xs:date" />
                            <!-- Section 4.4.3.2.4 Transaction Element -->
                            <xs:element name="Transaction">
                              <xs:complexType>
                                <xs:sequence>
                                  <xs:element name="StartDate" type="xs:date" />
                                  <xs:element name="EndDate" type="xs:date" />
                                </xs:sequence>
                              </xs:complexType>
                            </xs:element>
                          </xs:choice>
                          <xs:element name="NonRemitIndicator" type ="IndicatorType" minOccurs="0" />
                          <xs:element name="Taxable" type ="IndicatorType" minOccurs="0" />
                          <xs:element name="TaxForm1099IssuedDate" type="xs:date" minOccurs="0" />
                          <xs:element name="Description" minOccurs="0">
                            <xs:simpleType>
                              <xs:restriction base="xs:string">
                                <xs:maxLength value="250" />
                              </xs:restriction>
                            </xs:simpleType>
                          </xs:element>
                          <!-- Section 4.4.3.2.5 AlternateLocation Element -->
                          <xs:element name="AlternateLocation" minOccurs="0">
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="Identifier" minOccurs="0">
                                  <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                      <xs:maxLength value="20" />
                                    </xs:restriction>
                                  </xs:simpleType>
                                </xs:element>
                                <xs:choice>
                                  <xs:element name="USAddress" type="USAddressType" />
                                  <xs:element name="ForeignAddress" type="ForeignAddressType" />
                                </xs:choice>
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                          <!-- Section 4.4.3.2.6 OFAC Element -->
                          <xs:element name="OFAC" minOccurs="0">
                            <xs:complexType>
                              <!-- TODO:
                              If this information is present, are all elements required? If not, which ones are?  As
                              currently defined here, they would all be required or the XML would fail validation.
            
                              KGW - I think we should query New York on the field sizes and what is required.
                              -->
                              <xs:sequence>
                                <xs:element name="DateOfBlocking" type="xs:date" minOccurs="0" />
                                <xs:element name="LegalAuthority" minOccurs="0">
                                  <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                      <xs:maxLength value="100" />
                                    </xs:restriction>
                                  </xs:simpleType>
                                </xs:element>
                                <xs:element name="SanctionsTarget">
                                  <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                      <xs:maxLength value="100" />
                                    </xs:restriction>
                                  </xs:simpleType>
                                </xs:element>
                                <!-- Section 4.4.3.2.6.1 OFAC OwnerType Element -->
                                <xs:element name="OwnerType" minOccurs="0">
                                  <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                      <xs:maxLength value="50" />
                                      <xs:enumeration value="Individal" />
                                      <xs:enumeration value="U.S. Bank" />
                                      <xs:enumeration value="Non-U.S. Bank" />
                                      <xs:enumeration value="U.S. Non-Bank" />
                                      <xs:enumeration value="Non-U.S. Non-Bank" />
                                    </xs:restriction>
                                  </xs:simpleType>
                                </xs:element>
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                          <xs:element name="HIPAAIndicator" type="IndicatorType" minOccurs="0" />
                          <!-- Section 4.4.3.2.7 Owner Element -->
                          <xs:element name="Owner" minOccurs ="1" maxOccurs="unbounded">
                            <xs:complexType>
                              <xs:sequence>
                                <!-- Section 4.4.3.2.7.1 Owner TypeCode Element -->
                                <xs:element name="TypeCode">
                                  <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                      <xs:maxLength value="15" />
                                      <xs:enumeration value="Aggregate" />
                                      <xs:enumeration value="NamedOwner" />
                                      <xs:enumeration value="Unknown" />
                                    </xs:restriction>
                                  </xs:simpleType>
                                </xs:element>
                                <!-- Section 4.4.3.2.7.2 Identity Element -->
                                <xs:element name="Identity" minOccurs="0">
                                  <xs:complexType>
                                    <xs:sequence>
                                      <xs:choice>
                                        <!-- Section 4.4.3.2.7.2.1 USTaxID Element -->
                                        <xs:element name="USTaxID" minOccurs="0">
                                          <xs:complexType>
                                            <xs:sequence>
                                              <!-- Section 4.4.3.2.7.2.1.1 USTaxID TypeCode Element -->
                                              <xs:element name="TypeCode">
                                                <xs:simpleType>
                                                  <xs:restriction base="xs:string">
                                                    <xs:maxLength value="10" />
                                                    <xs:enumeration value="SSN" />
                                                    <xs:enumeration value="FEIN" />
                                                    <xs:enumeration value="ITIN" />
                                                  </xs:restriction>
                                                </xs:simpleType>
                                              </xs:element>
                                              <xs:element name="Number">
                                                <xs:simpleType>
                                                  <xs:restriction base="xs:string">
                                                    <xs:pattern value="[0-9]{9}" />
                                                  </xs:restriction>
                                                </xs:simpleType>
                                              </xs:element>
                                            </xs:sequence>
                                          </xs:complexType>
                                        </xs:element>
                                        <xs:element name="AlienID" minOccurs="0">
                                          <xs:simpleType>
                                            <xs:restriction base="xs:string">
                                              <xs:maxLength value="20" />
                                            </xs:restriction>
                                          </xs:simpleType>
                                        </xs:element>
                                      </xs:choice>
                                      <!-- Section 4.4.3.2.7.2.2 DriversLicense Element -->
                                      <xs:element name="DriversLicense" minOccurs="0">
                                        <xs:complexType>
                                          <xs:sequence>
                                            <xs:element name="State" type="StateCodeType" />
                                            <xs:element name="Identifier">
                                              <xs:simpleType>
                                                <xs:restriction base="xs:string">
                                                  <xs:maxLength value="15" />
                                                </xs:restriction>
                                              </xs:simpleType>
                                            </xs:element>
                                          </xs:sequence>
                                        </xs:complexType>
                                      </xs:element>
                                    </xs:sequence>
                                  </xs:complexType>
                                </xs:element>
                                <xs:element name="Contact" type="ContactType" />
                                <xs:element name="Description" minOccurs="0">
                                  <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                      <xs:maxLength value="255" />
                                    </xs:restriction>
                                  </xs:simpleType>
                                </xs:element>
                                <xs:element name="DateOfBirth" type="xs:date" minOccurs="0" />
                                <!-- Section 4.4.3.2.7.3 RelationshipCode Element -->
                                <xs:element name="RelationshipCode">
                                  <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                      <xs:maxLength value="50" />
                                      <xs:enumeration value="AD" />
                                      <xs:enumeration value="AG" />
                                      <xs:enumeration value="AF" />
                                      <xs:enumeration value="AN" />
                                      <xs:enumeration value="CP" />
                                      <xs:enumeration value="CN" />
                                      <xs:enumeration value="CF" />
                                      <xs:enumeration value="DF" />
                                      <xs:enumeration value="ES" />
                                      <xs:enumeration value="EX" />
                                      <xs:enumeration value="FB" />
                                      <xs:enumeration value="GR" />
                                      <xs:enumeration value="HE" />
                                      <xs:enumeration value="IN" />
                                      <xs:enumeration value="JT" />
                                      <xs:enumeration value="TC" />
                                      <xs:enumeration value="JE" />
                                      <xs:enumeration value="PC" />
                                      <xs:enumeration value="OR" />
                                      <xs:enumeration value="OT" />
                                      <xs:enumeration value="PD" />
                                      <xs:enumeration value="PA" />
                                      <xs:enumeration value="PO" />
                                      <xs:enumeration value="RE" />
                                      <xs:enumeration value="SO" />
                                      <xs:enumeration value="TE" />
                                      <xs:enumeration value="UG" />
                                      <xs:enumeration value="UT" />
                                      <xs:enumeration value="UN" />
                                      <xs:enumeration value="UF" />
                                      <xs:enumeration value="LA" />
                                    </xs:restriction>
                                  </xs:simpleType>
                                </xs:element>
                                <xs:element name="CompleteAccountRegistrationName" minOccurs="0">
                                  <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                      <xs:maxLength value="255" />
                                    </xs:restriction>
                                  </xs:simpleType>
                                </xs:element>
                                <xs:element name="DateOfTrust" type="xs:date" minOccurs="0" />
                                <xs:element name="MinorDateOfMajority" type="xs:date" minOccurs="0" />
                                <!-- Section 4.4.3.2.7.4 Deceased Element -->
                                <xs:element name="Deceased" minOccurs="0">
                                  <xs:complexType>
                                    <xs:sequence>
                                      <xs:element name="DateOfDeath" type="xs:date" minOccurs="0" />
                                      <xs:element name="DateOfDeathSource" minOccurs="0">
                                        <xs:simpleType>
                                          <xs:restriction base="xs:string">
                                            <xs:maxLength value="50" />
                                          </xs:restriction>
                                        </xs:simpleType>
                                      </xs:element>
                                      <xs:element name="DateOfDeathIdentifiedDate" type="xs:date" minOccurs="0" />
                                    </xs:sequence>
                                  </xs:complexType>
                                </xs:element>
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element> <!-- Owner -->
                          <!-- Section 4.4.3.2.8 Beneficiary Element -->
                          <xs:element name="Beneficiary" minOccurs="0" maxOccurs="unbounded">
                            <xs:complexType>
                              <xs:sequence>
                                <!-- Section 4.4.3.2.8.1 Beneficiary TypeCode Element -->
                                <xs:element name="TypeCode">
                                  <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                      <xs:maxLength value="50" />
                                      <xs:enumeration value="Primary" />
                                      <xs:enumeration value="Contingent" />
                                    </xs:restriction>
                                  </xs:simpleType>
                                </xs:element>
                                <xs:element name="Spouse" type="IndicatorType" minOccurs="0" />
                                <xs:element name="Percentage" type="PercentageType" />
                                <xs:element name="Contact" type="ContactType" />
                                <xs:element name="DateOfBirth" type="xs:date" minOccurs="0" />
                                <xs:element name="SSN" type="SSNType"  minOccurs="0" />
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element> <!-- Beneficiary -->
                          <!-- Section 4.4.3.2.9 Property Adjustment Element -->
                          <xs:element name="Adjustment" minOccurs="0" maxOccurs="unbounded">
                            <xs:complexType >
                              <xs:sequence>
                                <!-- Section 4.4.3.2.9.1 Property Adjustment TypeCode Element -->
                                <xs:element name="TypeCode">
                                  <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                      <xs:maxLength value="50" />
                                      <xs:enumeration value="DC" />
                                      <xs:enumeration value="SC" />
